{
  "name": "frontend",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "cd ../shared && npm run build && cd ../frontend && tsc && vite build",
    "lint": "eslint src --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,scss}\"",
    "type-check": "tsc --noEmit",
    "test": "node --no-warnings node_modules/vitest/vitest.mjs run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "preview": "vite preview",
    "clean": "rm -rf dist && rm -rf node_modules",
    "prepare": "npm run build && npm run type-check",
    "msw:generate": "msw init public --save",
    "check:all": "npm run check:duplicates && npm run check:deadcode",
    "check:duplicates": "jscpd src/ --ignore \"**/*.test.{ts,tsx}\" --reporters html,console --output reports/duplicates",
    "check:deadcode": "eslint \"./src/**/*.{ts,tsx}\" --max-warnings 0",
    "check:unused": "ts-prune -p tsconfig.json",
    "find:unused-files": "find-unused-exports src/main.tsx src/App.tsx --ext .ts,.tsx,.js,.jsx --ignore \"**/*.test.{ts,tsx}\" \"**/*.spec.{ts,tsx}\" \"**/types/**\""
  },
  "dependencies": {
    "@jinshanshan/shared": "file:../shared",
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@hookform/resolvers": "3.10.0",
    "@mui/icons-material": "5.16.9",
    "@mui/material": "5.16.9",
    "@mui/styled-engine": "6.4.3",
    "@mui/system": "6.4.3",
    "@reduxjs/toolkit": "2.0.1",
    "@types/yup": "0.29.14",
    "axios": "1.7.9",
    "axios-retry": "4.5.0",
    "crypto-js": "4.2.0",
    "framer-motion": "11.18.1",
    "lodash": "4.17.21",
    "lodash-es": "4.17.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.54.2",
    "react-redux": "9.0.4",
    "react-router-dom": "6.20.1",
    "redux": "5.0.1",
    "redux-logger": "3.0.6",
    "redux-persist": "6.0.0",
    "uuid": "11.0.5",
    "yup": "1.6.1",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@eslint/js": "^8.57.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.5.1",
    "@types/crypto-js": "^4.2.2",
    "@types/css-mediaquery": "^0.1.4",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.14",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.13.1",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@types/testing-library__react": "^10.0.1",
    "@types/testing-library__user-event": "^4.1.1",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^1.0.4",
    "css-mediaquery": "^0.1.2",
    "depcheck": "^1.4.7",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.8.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-perfectionist": "^4.8.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-sonarjs": "^3.0.1",
    "eslint-plugin-testing-library": "^7.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "find-unused-exports": "^7.1.1",
    "husky": "^9.1.7",
    "ioredis": "^5.4.2",
    "jscpd": "^3.5.10",
    "jsdom": "^23.2.0",
    "lint-staged": "^15.4.3",
    "msw": "^2.0.11",
    "prettier": "^3.1.0",
    "rollup-plugin-visualizer": "^5.14.0",
    "ts-prune": "^0.10.3",
    "typescript": "^5.3.3",
    "vite": "^5.0.7",
    "vite-plugin-checker": "^0.8.0",
    "vitest": "^1.6.1"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
